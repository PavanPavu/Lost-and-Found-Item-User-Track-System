pipeline {
    agent any

    stages {
        stage('1.git checkout') {
            steps {
                git branch: 'main', credentialsId: 'github_creds', url: 'https://github.com/PavanPavu/devops'
            }
        }
        stage('2.pwd') {
            steps {
                sh 'pwd'
            }
        }
    }
}
